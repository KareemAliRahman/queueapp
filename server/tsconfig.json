{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    //for typeorm
    "lib": ["es5", "es6"],
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "outDir": "./build",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
